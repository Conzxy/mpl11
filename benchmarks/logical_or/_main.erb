#define BOOST_MPL11_NO_ASSERTIONS
#include <boost/mpl11/bool.hpp>

<%= "#include <boost/mpl11/detail/logical_or/#{which}.hpp>" %>


<%= (0...breadth).map { |breadth|
    xs = (0...depth-1).map { 'boost::mpl11::false_' } << 'boost::mpl11::true_'
    <<-EOS
    static_assert(boost::mpl11::detail::logical_or::#{which}<
        #{xs.join(',')}
    >::value, "");
    EOS
}.join("\n") %>